        draw_set_colour(c_aqua);
        if (i == 6)
        {
            draw_text(225, rolePos + (i * 92), ds_list_find_value(role, i));
        }
        else
        {
            draw_text(225, rolePos + (i * 80), ds_list_find_value(role, i));
        }
        
        draw_set_colour(c_white);
        if (i == 5)
